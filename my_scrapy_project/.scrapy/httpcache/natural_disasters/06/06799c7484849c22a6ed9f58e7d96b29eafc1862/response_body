  document.addEventListener('DOMContentLoaded', function () {
    const mobileMenu = document.querySelector('.et_mobile_menu');

    if (mobileMenu) {
      // Prevent page scrolling when interacting with the menu
      mobileMenu.addEventListener('touchstart', function (e) {
        e.stopPropagation();
      });

      // Ensure menu scrolls smoothly on touch
      mobileMenu.addEventListener('touchmove', function (e) {
        e.stopPropagation();
      });

      // Fix scroll issues specific to the homepage
      const isHomepage = document.body.classList.contains('home');
      if (isHomepage) {
        document.body.style.overflow = 'hidden'; // Prevent body scroll
        mobileMenu.style.overflowY = 'scroll'; // Allow menu to scroll
      }
    }
  });
</script>
