  src: url("https://i.forbesimg.com/assets/fonts/schnyder-cond/schnyder-cond-bold.woff2") format("woff2");
}
@font-face {
  font-family: Schnyder Cond S;
  font-style: normal;
  font-weight: 400;
  src: url("https://i.forbesimg.com/assets/fonts/schnyder-cond/schnyder-cond-light.woff2") format("woff2");
