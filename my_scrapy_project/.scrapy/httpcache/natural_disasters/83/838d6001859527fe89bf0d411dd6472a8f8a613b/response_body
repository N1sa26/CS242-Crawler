      meters[meters.length - 2].style.display = 'none';
    }

    // get current meter buttons
    const meterCloseButton = meterCurrent.querySelector(
      '.ribbon .close-button',
    );
    const meterCta = meterCurrent.querySelector('.ribbon .meter-cta');

    meterCloseButton.addEventListener('click', () => {
      meterCurrent.style.display = 'none';
      document.body.classList?.remove('meter-ribbon-visible');
    });

