<script>
    (function () {
        async function submitUserData() {
            const userEmailInput = emailInput.value;
            if (emailRegExp.test(userEmailInput)) {
                successScreen.style.display = 'flex';
                initialScreen.style.display = 'none';

                templateParams['userEmailInput'] = userEmailInput;
                if(geolocation !== 'US') {
                    templateParams.Announcements = document.querySelector('#add-announcements').checked;
                }

                window.Zephr.utils.updateCordialListData(templateParams);

                const submitUserData = {
                    event: 'analyticsEvent',
                    'event category': 'Editorial Newsletters',
                    'event action': `${templateParams.newsletter} | ${templateParams.outcomeLabel}`,
                    'event label': 'click-unknown-signup',
                };

                if (typeof window.Zephr.utils.nextPaintAndTaskCallback !== 'function') {
                    console.info("%cFalling back to regular tracking behavior", "color: blue; font-size: 12px;");
                    window.dataLayer.push(submitUserData);
                } else {
                    window.Zephr.utils.nextPaintAndTaskCallback(() => {
                        window.dataLayer.push(submitUserData);
                    });
                }
            } else if (userEmailInput === '') {
                showErrorMessage(true, 'Please enter your email');
                return;
            }
        }

        const emailInput = document.querySelector('.newsletter-email-input');
        const emailRegExp =
            window.Zephr?.utils?.emailRegex ?? /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        const errorMessage = document.querySelector(
            '.newsletter-inline-error-message',
        );
        const submitButton = document.querySelector(
            '.newsletter-email-submit-button',
        );
        const gRecaptcha = document.querySelector('.g-recaptcha');

        let successScreen = document.getElementById('success-screen');
        let initialScreen = document.getElementById('initial-screen');
        const learnMoreButtons = document.querySelectorAll('.newsletter-learn-more-button');

        const newsletter = document.querySelector('.variables .newsletter').innerHTML;
        const geolocation = document.querySelector(
            '.variables .geolocation',
        ).innerHTML;
        const lander = document.querySelector('.variables .lander').innerHTML;
        const additionalFieldKey = document.querySelector(
            '.variables .additional-field-key',
        ).innerHTML;
        const additionalFieldValue = document.querySelector(
            '.variables .additional-field',
        ).innerHTML;
        const featureName = 'inline-editorial-newsletters';
        const outcomeLabel = window.Zephr.outcomes[featureName]?.outcomeLabel;

        if(geolocation !== 'US' ) {
            const addAnnouncementsContainer = document.querySelector('.add-announcements-container');
            addAnnouncementsContainer.innerHTML = '<input id="add-announcements" type="checkbox"><label for="add-announcements">Get the latest news on special offers, product updates and content suggestions from Forbes and its affiliates.</label>'
        }

        const templateParams = {
            newsletter,
            geolocation,
            lander,
            additionalFieldKey,
            additionalFieldValue,
            outcomeLabel,
        };

        submitButton.disabled = true;
        const gPrivacyPolicy = '<a href="https://policies.google.com/privacy" target="_blank">Privacy Policy</a>';
        const gTermsOfService = '<a href="https://policies.google.com/terms" target="_blank">Terms of Service</a>';
        const googlCaptchaText = `Forbes is protected by reCAPTCHA, and the Google ${gPrivacyPolicy} and ${gTermsOfService} apply.`;
        const termsOfService = '<a href="https://www.forbes.com/terms-and-conditions" target="_blank">Terms of Service</a>';
        const privacyStatement = '<a href="https://www.forbes.com/privacy" target="_blank">Privacy Statement</a>';

        const disclosureText = `By signing up, you agree to ${geolocation !== 'US' ? `our ${termsOfService}, and you acknowledge our ${privacyStatement}.` : `receive this newsletter, other updates about Forbes and its affiliates offerings, our ${termsOfService} (including resolving disputes on an individual basis via arbitration), and you acknowledge our ${privacyStatement}.`} ${googlCaptchaText}`

        const disclosureElement = document.querySelector('#initial-screen .newsletter-footer') || {};
        disclosureElement.innerHTML = disclosureText;

        /** @function showErrorMessage
         * @param {boolean} isErrorMessageShowing
         * @param {string} message
         */
        const showErrorMessage = (isErrorMessageShowing, message) => {
            errorMessage.innerText = message;
            if (isErrorMessageShowing) {
                emailInput.classList.add('email-error');
            } else {
                emailInput.classList.remove('email-error');
            }
        };

        /* EventListener that removes the error message and disables the submit
        button when the input field is not on focus. */
        emailInput.addEventListener('blur', () => {
            if (emailInput.value === '') {
                showErrorMessage(false, '');
            }
        });

        emailInput.addEventListener('click', () => {
            const emailInputData = {
                event: 'analyticsEvent',
                'event category': 'Editorial Newsletters',
                'event action': `${templateParams.newsletter} | ${templateParams.outcomeLabel}`,
                'event label': 'click-email-address',
            };

            if (typeof window.Zephr.utils.nextPaintAndTaskCallback !== 'function') {
                console.info("%cFalling back to regular tracking behavior", "color: blue; font-size: 12px;");
                window.dataLayer.push(emailInputData);
            } else {
                window.Zephr.utils.nextPaintAndTaskCallback(() => {
                    window.dataLayer.push(emailInputData);
                });
            }
        });

        /* EventListener to enable / disable error the message notification based on users input  */
        emailInput.addEventListener('input', () => {
            if (emailRegExp.test(emailInput.value)) {
                showErrorMessage(false, '');
                submitButton.disabled = false;
            } else {
                showErrorMessage(true, 'Please enter a valid email');
                submitButton.disabled = true;
            }
        });

        window.gRecaptchaCallback = function (token) {
            window.forbes.grecaptcha = token;
            submitUserData();
        };

        submitButton.addEventListener('click', (e) => {
            e.preventDefault(); // Prevent the default form submission
            e.stopPropagation();
            if (gRecaptcha) {
                window.grecaptcha.execute();
            }
        });

        learnMoreButtons.forEach((button) => {
            button.addEventListener('click', () => {
                window.Zephr.utils.nextPaintAndTaskCallback(() => { 
                    window.dataLayer.push({
                        event: 'analyticsEvent',
                        'event category': 'Editorial Newsletters',
                        'event action': `${templateParams.newsletter} | ${templateParams.outcomeLabel}`,
                        'event label': 'click-unknown-more-newsletters',
                    });
                });
            });
        })

        window.Zephr.utils.nextPaintAndTaskCallback(() => { 
            window.dataLayer.push({
                event: 'analyticsEvent',
                'event category': 'Editorial Newsletters',
                'event action': `${templateParams.newsletter} | ${templateParams.outcomeLabel}`,
                'event label': 'shown',
            });
        });
    })();
</script><script src="https://www.google.com/recaptcha/api.js" async="" defer=""></script><p>3<em>. Minnesota</em> (10.69% chance). Ranking third in terms of vulnerability to weather-related disasters, Minnesota has a 10.69% likelihood of wildfires, tornadoes or floods occurring in the upcoming years. Between 2010 and 2025, Minnesota had one of the highest levels of flood activity, with 11 major floods recorded and a 5.02% probability of future floods.</p>
<script>
    function loadConnatixScript(document) {
        if (!window.cnxel) {
            window.cnxel = {};
            window.cnxel.cmd = [];
            var iframe = document.createElement('iframe');
            iframe.style.display = 'none';
            iframe.onload = function() {
                var iframeDoc = iframe.contentWindow.document;
                var script = iframeDoc.createElement('script');
                script.src = '//cd.elements.video/player.js' + '?cid=' + '62cec241-7d09-4462-afc2-f72f8d8ef40a';
                script.setAttribute('defer', '1');
                script.setAttribute('type', 'text/javascript');
                iframeDoc.body.appendChild(script);
            };
            document.head.appendChild(iframe);

            const preloadResourcesEndpoint = 'https://cds.elements.video/a/preload-resources-ovp.json';
            fetch(preloadResourcesEndpoint, { priority: 'low' })
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok', preloadResourcesEndpoint);
                    }
                    return response.json();
                })
                .then(data => {
                    const cssUrl = data.css;
                    const cssUrlLink = document.createElement('link');
                    cssUrlLink.rel = 'stylesheet';
                    cssUrlLink.href = cssUrl;
                    cssUrlLink.as = 'style';
                    cssUrlLink.media = 'print';
                    cssUrlLink.onload = function() {
                        this.media = 'all';
                    };
                    document.head.appendChild(cssUrlLink);

                    const hls = data.hls;
                    const hlsScript = document.createElement('script');
                    hlsScript.src = hls;
                    hlsScript.setAttribute('defer', '1');
                    hlsScript.setAttribute('type', 'text/javascript');
                    document.head.appendChild(hlsScript);
                }).catch(error => {
                    console.error('There was a problem with the fetch operation:', error);
                });
        }
    }
    loadConnatixScript(document);
</script>
