<script>/**
 * Replaces 'REDIRECT_URL' with the current page href for button url template variable, and adds pixel tracking call on click
 * @param {Object[]} meterCTAs an array of meter CTAs in the page
 */
function updateMeterCTAUrls(meterCTAs = []) {
	meterCTAs.forEach((meterCTA) => {
		const currentHref = meterCTA.href || '';
		meterCTA.href = currentHref.includes('REDIRECT_URL')
			? currentHref.replace('REDIRECT_URL', encodeURIComponent(window.location.href))
			: currentHref;
		meterCTA.addEventListener('click', () => {
			window.fbq('track', 'InitiateCheckout');
		});
	});
}

updateMeterCTAUrls(document.querySelectorAll('.meter-cta'));
</script>
<script>
  try {
    // hide prev meter
    meters = document.querySelectorAll('.ribbon');
    metersOutcome = window.Zephr.outcomes.article.outcomeLabel;
    meterTemplate = meters[meters.length - 1];
    document.querySelector('.paywall_ribbon').appendChild(meterTemplate);

    // there may be two meters on the same page
    metersOnPage = document.querySelectorAll('.ribbon');
    meterCurrent = metersOnPage[metersOnPage.length - 1];

    // if this isn't the first meter to be shown, hide the previous one
    if (meters.length > 1) {
      meters[meters.length - 2].style.display = 'none';
    }

    // get current meter buttons
    meterCloseButton = meterCurrent.querySelector('.ribbon .close-button');
    meterCta = meterCurrent.querySelector('.ribbon .meter-cta');

    meterCloseButton.addEventListener('click', () => {
      meterCurrent.style.display = 'none';
      document.body.classList?.remove('meter-ribbon-visible');
    });

    const shownData = {
      event: 'analyticsEvent',
      'event category': 'UA',
      'event action': metersOutcome,
      'event label': 'shown',
		};

    if (typeof window.Zephr.utils.nextPaintAndTaskCallback !== 'function') {
      console.info("%cFalling back to regular tracking behavior", "color: blue; font-size: 12px;");
      window.dataLayer.push(shownData);
    } else {
      window.Zephr.utils.nextPaintAndTaskCallback(() => {
        window.dataLayer.push(shownData);
      });
    }

    meterCta.addEventListener('click', () => {
      const subscribeData = {
        event: 'analyticsEvent',
        'event category': 'UA',
        'event action': metersOutcome,
        'event label': 'click-subscribe',
      };

      if (typeof window.Zephr.utils.nextPaintAndTaskCallback !== 'function') {
        console.info("%cFalling back to regular tracking behavior", "color: blue; font-size: 12px;");
        window.dataLayer.push(subscribeData);
      } else {
        window.Zephr.utils.nextPaintAndTaskCallback(() => {
          window.dataLayer.push(subscribeData);
        });
      }
    });

    meterCloseButton.addEventListener('click', () => {
      const closeMeterData = {
        event: 'analyticsEvent',
        'event category': 'UA',
        'event action': metersOutcome,
        'event label': 'click-closemeter',
      };

      if (typeof window.Zephr.utils.nextPaintAndTaskCallback !== 'function') {
        console.info("%cFalling back to regular tracking behavior", "color: blue; font-size: 12px;");
        window.dataLayer.push(closeMeterData);
      } else {
        window.Zephr.utils.nextPaintAndTaskCallback(() => {
          window.dataLayer.push(closeMeterData);
        });
      }
    });
  } catch (error) {
