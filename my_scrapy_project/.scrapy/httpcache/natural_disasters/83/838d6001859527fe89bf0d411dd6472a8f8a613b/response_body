<script>
	const options = {
		formId: {
			guest: '01J2XX5NWABRJEPKRATES6S3TE',
			loggedInUser: '01J2XX5NWABRJEPKRATES6S3TE',
			premiumUser: '01J2XX5NWABRJEPKRATES6S3TE',
		},
		percentOfUsers: 1,
		trackingLabel: 'wealth-segments-survey',
	};

	(() => {
		const forceShowSurvey = /[?&]force-show-survey=/.test(location.search);
		const isPremium = window.Zephr?.utils?.doesUserHaveProduct(window.Zephr?.utils?.productMaps?.['unlimitedSubscriptionProducts']);

		let userType = 'guest';
		if (!!window.Zephr?.user) userType = 'loggedInUser';
		if (isPremium) userType = 'premiumUser';

		const formId = options.formId?.[userType] || options.formId?.['guest'];

		const cookieName = `TypeFormSeen-${formId}`;
		const formReadyFunctionName = `typeFormReady${formId}`;

		const hasExclusionCookie = (document.cookie.split(';').some((cookie) => cookie.includes(cookieName)));
		const fvid = document.cookie?.split(';').map((c) => c.trim().match(/^client_id=(.+)$/)?.[1]).filter(Boolean)[0];

		const currentScriptParentElement = document.currentScript.parentElement;
		const parentModal = currentScriptParentElement.querySelector('.typeform-container');

		const hideSurvey = options.percentOfUsers ? Math.random() > options.percentOfUsers / 100 : false;

		if ((hasExclusionCookie || hideSurvey) && !forceShowSurvey) {
			parentModal.setAttribute('exclusive-action-mode', 'excluded');
		}

		parentModal.show = () => {
			window[formReadyFunctionName] = () => {
				// open the Zephr modal
				window.Zephr.utils?.showModal('survey');

				// add tracking
				if (options.trackingLabel) {
					window.dataLayer.push({
						event: 'analyticsEvent',
						'event category': 'Template Area Interaction',
						'event action': 'in-view',
						'event label': options.trackingLabel,
					});
				}

				// set the cookie
				const expiry = new Date();
				expiry.setFullYear(expiry.getFullYear() + 1);

				document.cookie = `${cookieName}=1; Path=/; Domain=.forbes.com; expires=${expiry.toUTCString()};`;
			};

			parentModal.innerHTML = `<div class="typeform" data-tf-live="${formId}" data-tf-hidden="fvid=${fvid}" data-tf-on-ready="${formReadyFunctionName}"></div>`;

			const script = document.createElement('script');
			script.src = '//embed.typeform.com/next/embed.js';
			document.body.appendChild(script);
		};
	})();
</script><style></style>

<script>
	(function () {
		const priority = '1';

		// Target trigger types
		const immediately = 'false';
		const exitIntentTop = 'false';
		const exitIntentRight = 'false';
		const exitIntentLeft = 'false';
		const exitIntentBottom = 'false';
		const exitIntentMobile = 'false';
		const scrollPercentage = '';
		const timeDelay = '10s';
		const never = 'false';

		const triggerOptions = {
			immediately,
			exitIntentTop,
			exitIntentRight,
			exitIntentLeft,
			exitIntentBottom,
			exitIntentMobile,
			scrollPercentage,
			timeDelay,
			never
		};

		window.Zephr.utils?.exclusiveActions?.register(document.currentScript, triggerOptions, priority);
	})();
