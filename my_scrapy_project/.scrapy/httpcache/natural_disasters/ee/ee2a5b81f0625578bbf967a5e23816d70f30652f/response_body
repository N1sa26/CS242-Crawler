  #con8461.collapsed button.collapser { top: 0.25em; right: 0.25em; font-size: 10em; }
  #con8461 button.collapser:focus { outline: 2px dotted #ec1300; }

  #con8461 button.collapser svg {
    width: 1.75em;
    vertical-align: middle;
    overflow: hidden;
  }
  #con8461 button.collapser svg:hover { color: black; }
  .desktopLastChance {font-size: 28em; line-height: 1.18; font-weight: 900; padding-bottom: 0.6em;}
  .collapsed .desktopLastChance, .mobile .desktopLastChance { display: none!important;}
</style><script id="con8461-script">
try {
  (function () {
    'use strict';
    var TNT_CAMPAIGN_CON8461 = {
      VERSION: 'Fri Dec 22 12:38:02 EST 2023',
      CR_NBR: 'CON8461',
      CID: '606522',
      CAMPAIGN: 'MEM-11535 | BAU Price Change SWAT Copy Change | ANON | PROD',
      ACTIVITY_NAME: 'MEM-11535 | BAU Price Change SWAT Copy Change | ANON | PROD',
      EXPERIENCE_ID: '1',
      EXPERIENCE: 'SWAT',
      EXPERIENCE_NAME: '',
      TRAFFIC_ALLOCATION_ID: '',
      TRAFFIC_ALLOCATION_TYPE: '',
      OFFER_ID: '1155491',
      OFFER: 'PROD-MEM-QTR-PC-INTS-ANON',
      MBOX: 'target-global-mbox-aarp',
      PATHNAME: '',
      //DEVICE_TYPE: '',
      DEVICE: '',
      MOBILE_DEVICE: '', // '1' for Phone or Tablet.
      MOBILE_PHONE: '', // '1' for Phone.

      CONFIG: {
        FG_START: new Date('2023-03-31T00:00-04:00'),
        FG_END: new Date('2023-05-01T00:00:00-04:00'),
        VARIANT: 'OG', // Original = OG, Free gift = FG
        STATE: 'COL',
        CTA: {
          J: 'https://join.aarp.org/joinnav',
          R: 'https://join.aarp.org/rfrenew',
        },
        INTCMP: 'ATT-MEM-11535',
        V: 'v2',
        CREATIVE: 'WYG',
        CID: {
          MBCH: {
            J: 'ULC24W2',
            R: 'ZLC24W2',
          }
        },
      },

      processFirstVisit() {
        const cookieName = 'con8461-swat-fv',
          nowDt = new Date(),
          now = nowDt.getTime(),
          fvThreshold = now + 5000;
        function getCookie() {
          const kv = document.cookie.split('; ').filter(e => e.startsWith(cookieName+'='));
          if (kv.length) {
            const v = kv[0].split('=')[1];
            try {
              return Number(v);
            } catch (ex) {
              // Ignore the exception
            }
          }
          return fvThreshold;
        }
        function setCookie() {
          const midnight = new Date(nowDt.setHours(24, 0, 0, 0));
          document.cookie = cookieName + '=' + fvThreshold +
            '; expires=' + midnight.toUTCString() +
            '; path=/';
        }
        const fv = now < getCookie();
        if (!document.cookie.includes(cookieName+'=')) setCookie();
        return fv;
      },

      init: function () {
        const now = new Date;
        if (now >= this.CONFIG.FG_START && now < this.CONFIG.FG_END) this.CONFIG.VARIANT = 'FG';

        this.EXPERIENCE = this.EXPERIENCE.toLowerCase();

        this.FIRSTVISIT = this.FIRSTVISIT_OVERRIDE || this.processFirstVisit();

        this.CONFIG.VARIANT = this.VARIANT_OVERRIDE || this.CONFIG.VARIANT;

        if (window.location.pathname.includes('/benefits-discounts/')) {
          this.MBCH = true;
        } else if (window.location.pathname.includes('/membership/benefits/')) {
          this.MBCH = true;
        }

        if (this.FIRSTVISIT) this.CONFIG.STATE = 'EXP';

        if (this.MBCH) this.CONFIG.STATE = 'COL';

        const A = window.AARP,
          memstatus = A && A.MetaInfo && A.MetaInfo.userData && A.MetaInfo.userData.memstatus || '';

        switch (memstatus) {
          case '': // Anonymous
            this.SEGMENT = 'ANON';
            break;
          default:
          case '1': // Non-member
          case 'U': // Suspended member (undeliverable address suspend)
          case 'D': // Suspended member (deceased member suspend)
            this.SEGMENT = 'NONM';
            break;
		  case '4': // Expired
          case '5': // Cancelled
			this.SEGMENT = 'EXPM';
            break;
          case '0': // Active Member
          case 'M': // Suspended member (member requested suspend)
            return;
        }

        this.SEGMENT = this.SEGMENT_OVERRIDE || this.SEGMENT;

        document.documentElement.classList.add('tnt-con8461');

        const div = document.createElement('div');
        div.innerHTML = this.getHtml();
        document.body.append(div.firstElementChild);

        this.device();
        if (this.STICKY === undefined)
          this.STICKY = this.DEVICE === 'desktop';

        if (this.DEVICE !== 'desktop')
          this.CONFIG.STATE = 'COL';

        this.tryCampaign(this);
      },

      tryCampaign: (thisObj, invoked=0) => {
        try {
          if (invoked < 15)
            thisObj.runCampaign() || setTimeout(thisObj.tryCampaign, 200, thisObj, invoked+1);
          else
            console.error('CON-8461: campaign failed to run');
        } catch(ex) {
          console.error('CON-8461: failed:', ex);
        }
      },

      runCampaign() {
        if (typeof _satellite != 'object') { return false; }
        this.CHANNEL = _satellite.getVar('ANA: Custom Channel Section').toUpperCase();

        if (this.MBCH && this.CONFIG.CID && this.CONFIG.CID.MBCH) {
          Object.assign(this.CONFIG.CID, this.CONFIG.CID.MBCH);
        }

        return this.runExperience();
      },

      runExperience() {
        const {wrapper, uxdia, afterWrapper} = this.findWrapper();
        if (uxdia) this.STICKY = false;

        if (!wrapper) {
          return false;
        }

        const container = this.createContainer(uxdia);
        if (!container) {
          return false;
        }

        if (this.CONFIG.STATE === 'EXP' && this.STICKY) {
          const autoCollapse = () => {
            if (this.CONFIG.STATE === 'EXP') {
              const { height, bottom } = container.getBoundingClientRect(),
                amtVisible = Math.max((bottom - 60) / height, 0);
              if (amtVisible === 0) {
                for (const btn of container.querySelectorAll('button.collapser'))
                  btn.click();
              }
            } else {
              window.removeEventListener('scroll', autoCollapse);
            }
          };
          window.addEventListener('scroll', autoCollapse);
        }

        if (uxdia) {
          if (afterWrapper)
            wrapper.after(container);
          else
            wrapper.appendChild(container);
        } else {
          wrapper.insertBefore(container, wrapper.firstElementChild);
          if (this.STICKY && this.CONFIG.STATE !== 'EXP')
            this.moveToStickyHeader(container);
        }

        this.adaptImagesAndLazyLoad();

        const resizeFn = () => {
          const container = document.getElementById('con8461');
          if (container.classList.contains('closed')) {
            window.removeEventListener('resize', resizeFn);
          } else {
            this.handleResize(this);
          }
        };

        resizeFn();
        window.addEventListener('resize', resizeFn);

        // On Safari, it can take some time for the layout to stablize
        setTimeout(resizeFn, 50);
        setTimeout(resizeFn, 200);
        setTimeout(resizeFn, 1000);

        return true;
      },

      findWrapper() {
        let wrapper, wrappers;

        wrapper = document.querySelector('#container_membership_banner > div');
        if (wrapper) return {wrapper, uxdia: true, afterWrapper: false};

        wrapper = document.querySelector('div.aarpe-main-content-wrap.container');
        if (wrapper) return {wrapper, uxdia: false};

        wrappers = document.querySelectorAll('header.container > div.cmp-container > div.experiencefragment');
        if (wrappers.length > 0) {
          if (this.MBCH)
            wrapper = wrappers[0];
          else
            wrapper = wrappers[wrappers.length-1];
        }

        if (wrapper) return {wrapper, uxdia: true, afterWrapper: true};

        wrapper = document.querySelector('header.container div.cmp-container div.aem-Grid');
        if (wrapper) return {wrapper, uxdia: true, afterWrapper: false};

        if (!wrapper) {
          console.error('CON-8461: wrapper not found:', wrapper);
          return false;
        }
        return wrapper;
      },

      createContainer(uxdia) {
        const container = document.getElementById('con8461'),
          uxdiaContainer = document.getElementById('con8461-uxdia-container');

        if (!container) {
          console.error('CON-8461: container not found:', 'con8461');
          return false;
        }

        if (uxdia) {
          container.classList.add('con8461-uxdia');
          container.classList.add('cmp-container');
          container.parentElement.classList.add('con8461-uxdia');
        } else {
          container.classList.add('con8461-aarpe');
          container.parentElement.classList.add('con8461-aarpe');
        }

        container.classList.add(this.SEGMENT.toLowerCase());

        container.classList.add(this.CONFIG.V);

        container.classList.add(this.CONFIG.VARIANT.toLowerCase());

        // User collapses the banner
        for (const btn of container.querySelectorAll('button.collapser')) {
          btn.addEventListener('click', ev => {
            ev.preventDefault();
            ev.stopImmediatePropagation();
            if (container.classList.contains('expanded')) {
              this.CONFIG.STATE = 'COL';
              container.classList.remove('expanded');
              container.classList.add('collapsed');
              this.addButtonTitle(container);
              this.handleResize(this);
              this.nav(container, this.CONFIG.STATE);
              if (!uxdia && this.STICKY) this.moveToStickyHeader(container);
              btn.blur();
            } else {
              uxdiaContainer.classList.add('closed');
              container.classList.remove('collapsed');
              container.classList.add('closed');
              this.CONFIG.STATE = 'CLOSED';
            }
          });
        }

        this.nav(container, this.CONFIG.STATE);

        this.addButtonTitle(container);

        this.addLinkLabels(container);

        container.addEventListener('click', ev => {
          if (ev.target.closest('a, button')) return;
          const swatCta = container.querySelector('a.swat.cta');
          if (swatCta) {
            swatCta.click();
            ev.stopPropagation();
          }
        });

        return uxdia ? uxdiaContainer : container.parentElement;
      },

      getHtml() {
        return `
<div id="con8461-div" style="display: none;">
  <!-- VERSION: 'Fri Dec 22 12:38:02 EST 2023' -->
  <div id="con8461hw">
    <div id="con8461h" class="container">
    </div>
  </div>

  <div id="con8461-uxdia-container" class="responsivegrid aem-GridColumn aem-GridColumn--default--12">
    <div id="con8461-swat-container">
      <div id="con8461" class="aarp-hpto-banner collapsed">
        <div class="bg expanded desktop desktop-lg">
          <img fetchpriority='high' class="bg img-responsive" aria-hidden="true" data-image
            data-original-src="https://www.aarp.org/content/dam/aarp/members/2022/summer-of-swat-2022/wyg-2-1190-570-4.jpg"
            data-original-ratio="0.478991597" />
        </div>

        <div class="bg expanded desktop desktop-sm">
          <img fetchpriority='high' class="bg img-responsive" aria-hidden="true" data-image
            data-original-src="https://www.aarp.org/content/dam/aarp/members/2022/summer-of-swat-2022/wyg-1-768-384-4.jpg"
            data-original-ratio="0.5" />
        </div>

        <div class="bg collapsed desktop desktop-lg">
          <img fetchpriority='high' class="bg img-responsive" aria-hidden="true" data-image
            data-original-src="https://www.aarp.org/content/dam/aarp/members/2022/summer-of-swat-2022/wyg-2-1190-90-4.jpg"
            data-original-ratio="0.075630252" />
        </div>

        <div class="bg collapsed desktop desktop-sm">
          <img fetchpriority='high' class="bg img-responsive" aria-hidden="true" data-image
            data-original-src="https://www.aarp.org/content/dam/aarp/members/2022/summer-of-swat-2022/wyg-2-768-144-4.jpg"
            data-original-ratio="0.1875" />
        </div>

        <div class="bg mobile">
          <img fetchpriority='high' class="bg img-responsive" aria-hidden="true" data-image
            data-original-src="https://www.aarp.org/content/dam/aarp/members/2022/summer-of-swat-2022/benefits.jpg"
            data-original-ratio="0.322580645" />
        </div>

        <a class="swat cta" href="#" aria-label data-join-label-og="Join Today" data-join-label-fg="Join & Claim Gift"></a>

        <div class="swat-banner-container en">
          <div class="swat-banner">
            <div class="s1">
              <div class="hw">
                <p class="og">AARP Membership</p>
                <p class="fg">Join & Get a FREE GIFT!</p>
              </div>
