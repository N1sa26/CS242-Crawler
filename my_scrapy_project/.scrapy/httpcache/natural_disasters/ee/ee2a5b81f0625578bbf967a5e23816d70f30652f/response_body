var getAllVarsPageLoad = function() {
  var s = window.s;
  for (var i = 0; i < Object.keys(s).length; i++) {
    if (Object.keys(s)[i].indexOf('prop') != -1 || Object.keys(s)[i].indexOf('eVar') != -1) {
      if (s[Object.keys(s)[i]]) {
        s.linkTrackVars = AARP.analytics.custom.apl(s.linkTrackVars, Object.keys(s)[i], ",", 1);
      }
    }
  }
};
window.getAllVarsPageLoad = getAllVarsPageLoad;
