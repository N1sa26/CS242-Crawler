        </div>

        <button class="collapser"
          data-title="Collapse" data-title-expanded="Collapse" data-title-collapsed="Close">
          <svg class="svg-icon" viewBox="0 0 100 100" version="1.1" xmlns="http://www.w3.org/2000/svg">
            <g stroke="currentColor" stroke-width="10" stroke-linecap="round" fill="none">
              <circle cx="50" cy="50" r="40" stroke-width="5" fill="none" ></circle>
              <line x1="35" y1="35" x2="65" y2="65" ></line>
              <line x1="35" y1="65" x2="65" y2="35" ></line>
            </g>
          </svg>
        </button>
      </div>
    </div>
  </div>
</div>
        `;
      },

      addButtonTitle(container) {
        for (const el of container.querySelectorAll('button[data-title]')) {
          const title = el.dataset.title,
            titleExpanded = el.dataset.titleExpanded,
            titleCollapsed = el.dataset.titleCollapsed;
          if (container.classList.contains('expanded'))
            el.title = titleExpanded || title;
          else if (container.classList.contains('collapsed'))
            el.title = titleCollapsed || title;
          else
            el.title = title;
        }
      },

      addLinkLabels(container) {
        for (const e of container.querySelectorAll('a[aria-label]')) {
          const { joinLabelOg, joinLabelFg } = e.dataset,
            label = (this.CONFIG.VARIANT === 'FG') ? joinLabelFg : joinLabelOg;
          e.setAttribute('aria-label', label);
        }
      },

      handleResize(ctx) {
        const container = document.getElementById('con8461'),
          bgWidth = container.getBoundingClientRect().width,
          large = bgWidth > 767,
          closed = ctx.CONFIG.STATE === 'CLOSED',
          expanded = ctx.CONFIG.STATE === 'EXP',
          desktop = ctx.DEVICE !== 'mobile';

        let baseWidth, maxFs;

        if (desktop) {
          container.classList.add('desktop');
          container.classList.remove('mobile');
          if (large) {
            baseWidth = 1190;
            maxFs = 1200 / 1190;
            container.classList.add('desktop-lg');
            container.classList.remove('desktop-sm');
          } else {
            baseWidth = 768;
            maxFs = 1;
            container.classList.add('desktop-sm');
            container.classList.remove('desktop-lg');
          }
        } else {
          baseWidth = 310;
          maxFs = 767 / 310;
          container.classList.add('mobile');
          container.classList.remove('desktop');
          container.classList.remove('desktop-lg');
          container.classList.remove('desktop-sm');
        }

        if (desktop && !expanded && large) {
          container.classList.add('short');
          container.classList.remove('long');
        } else {
          container.classList.add('long');
          container.classList.remove('short');
        }

        if (closed) {
          container.classList.remove('expanded');
          container.classList.remove('collapsed');
          container.classList.add('closed');
        } else {
          if (expanded) {
            container.classList.remove('collapsed');
            container.classList.add('expanded');
          } else {
            container.classList.remove('expanded');
            container.classList.add('collapsed');
          }

          const fs = Math.min(bgWidth / baseWidth, maxFs);

          container.style.fontSize = fs + 'px';
        }

        ctx.adaptImagesAndLazyLoad();
      },

      nav(container, state) {
        const intCmp = (jrt, state) => {
          return [this.CONFIG.INTCMP, state, this.SEGMENT, this.CONFIG.V.toUpperCase(), jrt, this.CHANNEL].join('-');
        };

        const setAttributes = (el, href, id) => {
          el.setAttribute('href', href);
          el.setAttribute('data-formelementid', id);
        };

        const icj = intCmp('JOIN', state),
          icr = intCmp('RENEW', state),
          icb = intCmp('BANJ', state),
          //cidj = this.CONFIG.CID && this.CONFIG.CID.J,
          //cidr = this.CONFIG.CID && this.CONFIG.CID.R,
          //cidb = cidj,
          jurl = new URL(this.CONFIG.CTA.J),
          rurl = new URL(this.CONFIG.CTA.R),
          burl = new URL(this.CONFIG.CTA.J),
          jas = container.querySelectorAll('a.join.cta'),
          ras = container.querySelectorAll('a.renew.cta'),
          bas = container.querySelectorAll('a.swat.cta');

        icb && burl.searchParams.set('intcmp', icb);
        //cidb && burl.searchParams.set('campaignid', cidb);
        icj && jurl.searchParams.set('intcmp', icj);
        //cidj && jurl.searchParams.set('campaignid', cidj);
        icr && rurl.searchParams.set('intcmp', icr);
        //cidr && rurl.searchParams.set('campaignid', cidr);

        for (const a of bas)
          setAttributes(a, burl.href, icb + '-ID');
        for (const a of jas)
          setAttributes(a, jurl.href, icj + '-ID');
        for (const a of ras)
          setAttributes(a, rurl.href, icr + '-ID');
      },

      moveToStickyHeader(container, attempts=1, moveToStickyHeader) {
        moveToStickyHeader = moveToStickyHeader || this.moveToStickyHeader;
        const header = document.querySelector('header.aarpe-everywhere-header.navbar');
        if (header) {
          header.append(document.getElementById('con8461hw'));
          document.getElementById('con8461h').append(container);
        } else {
          if (attempts > 10)
            console.error('CON-8461: missing header');
          else {
            setTimeout(moveToStickyHeader, 200, container, attempts+1, moveToStickyHeader);
          }
        }
      },

      adaptImagesAndLazyLoad() {
        try {
          AARP.Foundation.AdaptImages.adapt();
          AARP.Foundation.LazyLoad.load();
        } catch(ex) {
          console.error('CON-8461: adaptive images and lazy loading failed:', ex);
        }
      },

      device: function () {
        this.DEVICE = this.MOBILE_DEVICE === '1' ? (this.MOBILE_PHONE === '1' ? 'mobile' : 'tablet') : 'desktop';
        this.DEVICE = this.DEVICE_OVERRIDE || this.DEVICE;

        document.documentElement.classList.add('tnt-' + this.DEVICE);
      },
    };

    TNT_CAMPAIGN_CON8461.init();
  })();
} catch(ex) {
  console.error('CON-8461: exception:', ex);
}
