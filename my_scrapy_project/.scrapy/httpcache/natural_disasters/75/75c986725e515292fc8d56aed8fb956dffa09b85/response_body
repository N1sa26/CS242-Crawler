</main><div class="page-bottom" style="left: auto; top: auto;"><div id="dfp-ad-oop2-wrapper" class="dfp-tag-wrapper dfp-ad-count"><div id="dfp-ad-oop2" class="dfp-tag-wrapper" data-google-query-id="CID53In1uosDFRZlCAQdCzAawA"><div id="google_ads_iframe_/22541732127/newsweek/oop2_0__container__" style="border: 0pt none; margin: auto; text-align: center;"></div></div></div><script>googletag.cmd.push(function () { if(googletag.slots["oop2"]) onDvtagReady(function(){ if (getDvtagTargeting().IDS?.[0] !== "1" || fqwl || nwdemocr!="") { googletag.display("dfp-ad-oop2"); }}); });</script><div class="block block-fusion-ads"><div class="content"><div id="dfp-ad-sticky-wrapper" class="dfp-tag-wrapper sticky_bottom dfp-ad-count" style="display:none;"><div id="close-sticky-btn" class="close-sticky-bottom" role="button" aria-label="Close"><svg width="16" height="16" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg" aria-label="Close" role="img"><path d="M15 1 1 15M1 1l14 14" stroke="#000" stroke-width="1.3" stroke-linecap="round" stroke-linejoin="round"></path></svg></div><div id="dfp-ad-sticky" class="dfp-tag-wrapper unstick"></div></div></div></div><script>googletag.cmd.push(function () { if(googletag.slots["sticky"]) onDvtagReady(function(){ if (getDvtagTargeting().IDS?.[0] !== "1" || fqwl || nwdemocr!="") { googletag.display("dfp-ad-sticky"); }}); });if(/sticky\|[0-1]\|0/.test(dfpSlots.pginfo[ibtmedia_device])) document.getElementById("dfp-ad-sticky").className += " unstick";</script> <script>var xh=0;googletag.cmd.push(function(){googletag.pubads().addEventListener('slotRenderEnded',function(event){var div=event.slot.getSlotElementId().split("-")[2];if(!event.isEmpty&&div=='sticky'&&(xuaInfo.country=="US"||xuaInfo.country=="CA"||xuaInfo.country=="AR")){xh=1;doFir.push(function(){document.body.classList.add('ad-sticky-bottom');const adStickyWrapper=document.getElementById('dfp-ad-sticky-wrapper');adStickyWrapper.style.display='block';document.getElementById('close-sticky-btn').addEventListener('click',function(){adStickyWrapper.style.display='none';adStickyWrapper.classList.remove('sticky_bottom');document.body.classList.remove('ad-sticky-bottom')})})}})})</script> <div class="container"><div class="col-md-9"><div id="recommended2"></div></div></div> <script>doFir.push(function(){var recommendO=document.getElementById('recommended');var recommended2=document.getElementById('recommended2');var blockContentRecirc=document.getElementById('block-nw-content-recirc');var eventName=decodeURIComponent('Recommended for you');var size=12;var placement='article-recs';if(recommendO){function loadRecommendations(){if(getCookie('_ga')){console.log('fetching recs');var requestUrl=blockContentRecirc.getAttribute('data-src')+'&ga='+getCookie('_ga')+'&type=&c_id='+dataLayer[0].sectionId+'&size='+size+'&placement='+placement+'&articleCols=4';fetch(requestUrl).then(function(response){return response.json()}).then(function(data){recommendO.innerHTML=data.html;if(recommended2){recommended2.style.minHeight='0'} var blockTitle=recommendO.querySelector('.block-title');if(data.label&&data.label[0]&&blockTitle){blockTitle.innerHTML=data.label[0]} var moreInElement=recommendO.querySelector('.more-in');if(moreInElement){setJsonData(moreInElement,data.items);analyticsEvent(eventName,'Open',location.href,'',!0);var moreInLinks=moreInElement.querySelectorAll('a');moreInLinks.forEach(function(link){link.addEventListener('click',function(){analyticsEvent(eventName,'Click',this.getAttribute('href'),'',!0)})})}}).catch(function(error){console.error('Error fetching data:',error)})}} var observerOptions={root:null,rootMargin:'100px',threshold:0.1};var observer=new IntersectionObserver(function(entries,observer){if(entries[0].isIntersecting){loadRecommendations();observer.unobserve(recommendO)}},observerOptions);observer.observe(recommendO)} hideElementsById(['readMoreTopics','newsletterSignupHorizontal']);processFirstParagraph();processArticleBodyChildren();checkSlideStart();removeEmbeddedTitles();processMobilePromoLink()});function hideElementsById(ids){ids.forEach(id=>{const element=document.getElementById(id);if(element)element.style.display='none'})}
function processFirstParagraph(){const firstP=document.querySelector('.article-body p');if(!firstP)return;const name=firstP.textContent.split(' ')[0];if(name.includes('.')){const node=document.getElementById('node-'+dataLayer[0].entityId);if(node)node.classList.remove('flc')}}
function processArticleBodyChildren(){const articleBody=document.querySelector('.article-body');const children=articleBody.children;const totalChildren=children.length;addTrendingArticlesRightRail('trending_articles');Array.from(children).forEach((child,index)=>{switch(index){case 6:insertElementHtml(child,'newsletterSignupHorizontal');break;case 9:break;case 12:addReadMoreComponent(child);break}});if(totalChildren<12){addReadMoreComponent(articleBody)}}
function insertPromoLink(element,index){if(!element){console.error('insertPromoLink: element is not a DOM node');return}
if(index==10){linkElement='<a class="promo-link signup-link" href="https://www.newsweek.com/preference-center?subref=incontent_web_'+index+'">Sign up for Newsweek’s daily headlines</a>'}else{linkText="Exclusively Available to Subscribers";linkTextCTA="Try it now for $1 ";linkElement='<div style="display:flex;justify-content:space-evenly;'+(mobile_device?'flex-wrap:wrap;':'')+'border-top:1px solid #c4c4c4;border-bottom:1px solid #c4c4c4;width: 100%;text-align: center;padding: 15px;">\
                            <img src="https://g.newsweek.com/www/img/voices_promo_link.svg" width=204 height=34>\
                            <b style="margin:5px; font-size:15px; color:#f61602;">'+linkText+'</b>\
                            <a href="https://www.newsweek.com/voices-description-page/?subref=incontent_web_'+index+'"\                               style="\                                   font: 900 15px Roboto,sans-serif !important; color:#FFF;background-color: #f61602 !important;\                                   border-radius: 2em !important;\                                   padding:0.5rem 2rem">'+linkTextCTA+'\
                            </a>\
                          </div>'}
const div=createElementWithHtml('div',linkElement);div.classList.add('promo-link');element.parentNode.insertBefore(div,element.nextSibling)}
function insertElementHtml(element,id){if(!element){console.error('insertElementHtml: element is not a DOM node');return}
if(document.getElementById(id)){const div=createElementWithHtml('div',document.getElementById(id).innerHTML);element.parentNode.insertBefore(div,element.nextSibling)}}
function createElementWithHtml(tag,html){const element=document.createElement(tag);element.innerHTML=html;return element}
function checkSlideStart(){if(!slide_started)check_slide()}
function removeEmbeddedTitles(){const embTIs=document.querySelectorAll('#v_article .embTI');embTIs.forEach(element=>element.remove());work_editor()}
function processMobilePromoLink(){if(!mobile_device||(xuaInfo.device!=='mobile'&&xuaInfo.device!==''))return;const promoLink=document.querySelector('.promo-link');const newsletterSignup=document.getElementById('newsletterSignupHorizontal');if(!promoLink){return}
if(!newsletterSignup){return}
if(!promoLink.parentNode){return}
promoLink.parentNode.insertBefore(newsletterSignup,promoLink.nextSibling)}
function addReadMoreComponent(element){if(document.querySelectorAll('.read-more-embedded').length===0&&document.getElementById('readMoreTopics')){fetch('https://d.newsweek.com/json/read-more-articles?article_id=2026499&article_link=should-americans-pay-insurance-risk-disaster-prone-states-2026499&article_category=News&source=by_parsely&article_keyword_related=8502&article_written=1739178001&size=4&days_old=4d&return=html&topic_label=Wildfires&topic_link=topic/wildfires').then(response=>response.json()).then(data=>{if(data&&data.html&&data.html.length>0){var readMoreTopicsElement=document.getElementById('readMoreTopics');readMoreTopicsElement.innerHTML=data.html;insertElementHtml(element,'readMoreTopics');readMoreTopicsElement.remove();let readmoreContainer=document.querySelector('.read-more-topics');let categoryGtm=document.querySelector('body').getAttribute('data-gtm-category');let actionGtm=readmoreContainer.getAttribute('data-gtm-action');let readmoreLinks=readmoreContainer.querySelectorAll('a');setGtmEventLinks(readmoreLinks,categoryGtm,actionGtm)}else{if(document.getElementById('readMoreTopics')){document.getElementById('readMoreTopics').remove()}}}).catch(error=>{console.error('Error fetching read more articles:',error)})}}
function addLatestArticlesRightRail(elementId){if(document.getElementById(elementId)){fetch('https://d.newsweek.com/json/get-latest-articles?gtm_source=Latest_Updates_Right_Trail&return=html').then(response=>response.json()).then(data=>{if(data!==undefined&&data.json.length>0){const element=document.getElementById(elementId);element.innerHTML=data.html;element.style.display='';let latestArticlesContainer=document.querySelector('.articles-single-column-container');let categoryGtm=document.querySelector('body').getAttribute('data-gtm-category');let actionGtm=latestArticlesContainer.getAttribute('data-gtm-action');let latestArticlesLinks=latestArticlesContainer.querySelectorAll('a');setGtmEventLinks(latestArticlesLinks,categoryGtm,actionGtm)}}).catch(error=>{console.error('Error fetching latest articles:',error)})}}
function getTrendingCommentsCount(data){const articles=data.json;Object.values(articles).forEach(({a_id})=>{if(typeof getMessageCount==='function'){getMessageCount(a_id,`article-${a_id}`,"comments")}})}
function addTrendingArticlesRightRail(elementId){if(document.getElementById(elementId)){fetch('https://d.newsweek.com/json/get-trending-articles?return=html&limit=5').then(response=>response.json()).then(data=>{if(data!==undefined&&data.json.length>0){const element=document.getElementById(elementId);element.innerHTML=data.html;element.style.display='';let trendingArticlesContainer=document.querySelector('.numbered-list-container');let categoryGtm=document.querySelector('body').getAttribute('data-gtm-category');let actionGtm=trendingArticlesContainer.getAttribute('data-gtm-action');let trendingArticlesLinks=trendingArticlesContainer.querySelectorAll('a');setGtmEventLinks(trendingArticlesLinks,categoryGtm,actionGtm);getTrendingCommentsCount(data)}}).catch(error=>{console.error('Error fetching trending articles:',error)})}}</script></div><footer class="footer-container">
