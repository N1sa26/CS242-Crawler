<span class="marker marker-right">Yes</span></div><div id="fairness-selected"><div class="selection-message"></div></div><a id="vote-button"> VOTE</a></div></div></section> <script>document.addEventListener('DOMContentLoaded',function(){const baseUrl='https://www.newsweek.com';const barWrapper=document.getElementById('bar-wrapper');const handle=document.getElementById('handle');const barfill=document.querySelectorAll('.bar-fill');const average_vote=document.getElementById('average_vote');const average_note=document.getElementById('average_note');const submitButton=document.getElementById('vote-button');const trustMeterOptionsIds=['540','543','546','549','552'];let po_id="4";let article_id="2026499";let isDragging=!1;let thevalue=null;const containerWidth=barWrapper.offsetWidth;const maxValue=4;const step=1;submitButton.disabled=!0;function calculateSteppedValue(position){const clampedPosition=Math.max(0,Math.min(containerWidth,position));const rawValue=(clampedPosition/containerWidth)*maxValue;return Math.round(rawValue/step)*step} function updateHandleAndValue(position){const steppedValue=calculateSteppedValue(position);const newPosition=(steppedValue/maxValue)*containerWidth;handle.style.left=newPosition+'px';getValueSelected(steppedValue)} handle.addEventListener('mousedown',function(){isDragging=!0});document.addEventListener('mouseup',function(){isDragging=!1});document.addEventListener('mousemove',function(event){if(isDragging){const rect=barWrapper.getBoundingClientRect();const relativeX=event.clientX-rect.left;updateHandleAndValue(relativeX)}});document.addEventListener('touchmove',function(event){if(isDragging){const touch=event.touches[0];const rect=barWrapper.getBoundingClientRect();const relativeX=touch.clientX-rect.left;updateHandleAndValue(relativeX)}});handle.addEventListener('touchstart',function(){isDragging=!0});document.addEventListener('touchend',function(){isDragging=!1});barfill.forEach((element,index)=>{element.addEventListener('click',(e)=>{handle.style.left=(index*25)+'%';getValueSelected(index)})});handle.addEventListener('click',(e)=>{enableVote()});function enableVote(){if(submitButton.disabled){submitButton.classList.add('enabled');submitButton.disabled=!1}}
function getValueSelected(selection){if(typeof trustMeterOptionsIds[selection]!=='undefined'){thevalue=trustMeterOptionsIds[selection];enableVote()}else{console.error("Error getting value for vote")}}
if(getCookie('poll_'+po_id+"_article_"+article_id)){getMeterResults(po_id,article_id)}else{submitButton?.addEventListener('click',(e)=>{if(thevalue==null){alert("Please choose an answer first!")}else{if(!getCookie('poll_'+po_id+"_article_"+article_id)){processMeter(po_id,article_id)}else{getMeterResults(po_id,article_id)}}})}
async function processMeter(po_id,article_id){const url=`${baseUrl}/tool/process-fairness-metter?pa_id=${thevalue}&po_id=${po_id}&article_id=${article_id}`;try{const response=await fetch(url);if(!response.ok)throw new Error('Network response was not ok');const data=await response.json();if(data.length===0)return!1;if(typeof analyticsEvent==="function"){let categoryMeter=document.querySelector('.fairness-metter').getAttribute('data-gtm-category');let action=document.querySelector('.fairness-metter').getAttribute('data-gtmAction');analyticsEvent(categoryMeter,action,thevalue,1,!0);console.log(categoryMeter,action,thevalue,1,!0)}}catch(error){console.error('There was a problem with the fetch operation:',error)}finally{getMeterResults(po_id,article_id)}}
async function getMeterResults(poll_id,article_id){const url=`${baseUrl}/tool/get-fairness-metter-results?po_id=${poll_id}&article_id=${article_id}`;try{const response=await fetch(url);if(!response.ok)throw new Error('Network response was not ok');const data=await response.json();if(data.length===0)return!1;handle.classList.add('voted');const averagePosition=(data.average-1)*25;average_vote.style.left=averagePosition+'%';average_vote.style.display='block';average_note.style.left=(averagePosition-9)+'%';average_note.style.display='block';const your_vote=(getCookie('poll_'+po_id+"_article_"+article_id));handle.style.left=(trustMeterOptionsIds.indexOf(your_vote)*25)+'%';handle.style.pointerEvents='none';barfill.forEach((element,index)=>{element.style.pointerEvents='none'});submitButton.classList.add('voted')
submitButton.innerHTML="COMMENT ON THIS ARTICLE";submitButton?.addEventListener('click',(e)=>{e.preventDefault();const element=document.querySelector('#vf-comment-block');if(element){const top=element.getBoundingClientRect().top+window.scrollY;window.scrollTo({top,behavior:'smooth'})}});const meterTitle=document.getElementById('meter-title');meterTitle.innerHTML="Thank you for voting!";meterTitle.style.color="#F72210";meterTitle.style.cursor="pointer"}catch(error){console.error('There was a problem with the fetch operation:',error)}finally{}}})</script> <div class="new-content-correction" data-gtm-action="Content_Correction"><div class="new-content-correction-links" id="artend_links">
