function debounce(func,wait,immediate){let timeout;return function(){const context=this,args=arguments;const later=function(){timeout=null;if(!immediate)func.apply(context,args)};const callNow=immediate&&!timeout;clearTimeout(timeout);timeout=setTimeout(later,wait);if(callNow)func.apply(context,args)}}
function initializeCustomScrollBehavior(){let paddingApplied=!1;let lastScrollPosition=0;const countParagraphs=()=>{const paragraphs=document.getElementsByTagName('p');let visibleCount=0;for(let i=0;i<paragraphs.length;i++){const paragraph=paragraphs[i];if(paragraph.offsetTop<window.scrollY+window.innerHeight){visibleCount++}} return visibleCount};const handleScroll=async()=>{if(barClosed){return}
