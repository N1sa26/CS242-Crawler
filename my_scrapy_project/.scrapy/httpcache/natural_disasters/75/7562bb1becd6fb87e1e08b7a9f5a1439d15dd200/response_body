    $().ready(function () {
        // The flag/report Modal
        $('body').on('click', '.flag_report_asset', function (e) {
            e.preventDefault();
            e.stopPropagation();

            var div_id = $(this).attr('id').split("_");
            var asset_type = div_id[0];
            $("input[id='replace_this_type']").filter(':hidden').val(asset_type);
            var modalHTML = $('#flag-reason-form').html();
            var modal = $.UIkit.modal.dialog(modalHTML.replace(/(\r\n|\n|\r)/gm, ""));
            modal.show();
        });

    });


</script>

