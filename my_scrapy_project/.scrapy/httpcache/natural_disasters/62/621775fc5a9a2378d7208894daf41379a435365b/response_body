    var filters = [], queryParamValue,section=[],sFilters="",sSection="";
    var query_string = window.location.search;
    var paramMap = {
        'displayconttype_exact': 'type',
        'topic_exact': 'topic',
        'admreg_exact':'region',
        'count_exact':'country',
        'strdate':'startdate',
        'enddate':'enddate',
