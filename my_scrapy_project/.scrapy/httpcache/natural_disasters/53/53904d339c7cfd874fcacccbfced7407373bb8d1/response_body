                                        {{publishDateAp}}
                                    </time>
                                </span>
                            </template>
                        </span>
                        <h3 class="title">
                            <span v-html="computedArticleTitle"></span>
                            <card-edit-span :isEditMode="canEdit" :editUrl="editUrl" :imageUrl="editImage"></card-edit-span>
                        </h3>
                        <p v-if="!hideSummary" class="summary hidden-xs hidden-xxs" v-html="articleSummary"></p>
                    </figcaption>
                </div>
                <a class="link-overlay" :href="articleUrlOrLink" :target="linkTarget" :rel="linkRel">
                    <span class="sr-only"  v-html="articleTitle"></span>
                </a>
            </div>
        </figure>
        <div style="clear:both"></div>
    </div>
</script>

