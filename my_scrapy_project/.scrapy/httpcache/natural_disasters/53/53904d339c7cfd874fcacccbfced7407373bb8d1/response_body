<script type="text/javascript">
    (function () {
        const DEBUG = false;
        if (DEBUG) console.log("m-i2: vue:initialize:storyListing");
        document.addEventListener("DOMContentLoaded", function () {
            if (DEBUG) console.log("initializing vue on storyListing in module 1282")
            const elTarg = "#storyListing-1282";

            // supports sending in "data" and "methods" by naming convention
            let modData = {};
            let modMethods = {};
            const typeofModData = typeof(storyListing1282_data);
            const typeofModMethods = typeof(storyListing1282_methods);

            if (typeofModData != "undefined") {
                modData = storyListing1282_data;
            }
            if (typeofModMethods != "undefined") {
                modMethods = storyListing1282_methods;
            }
            const el = $(elTarg)[0];
            const moduleVue = new Vue({
                el: el,
                mounted: function () {
                    // call the callback if exists.
                    // naming vueToken_mounted_1282
                    if (DEBUG) console.log("cb?", typeof (vueToken_mounted_1282));
                    if (typeof (vueToken_mounted_1282) === "function" ) {
                        const mountedCB = vueToken_mounted_1282;
                        if (mountedCB) {
                            // currently no arguments, just a time event, e.g. when slick
                            // could be initialized.
                            this.$nextTick(mountedCB);
                        }
                    }
                },
                data: modData,
                methods: modMethods,
            });

        });
    })();
</script>



                
