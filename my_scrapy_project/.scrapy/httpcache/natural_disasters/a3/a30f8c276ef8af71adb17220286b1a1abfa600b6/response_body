class EventMapper {

	_dataStore;

	constructor(dataStore) {
		this._dataStore = dataStore;
	}

	mapAAEvents(ev) {
		const evType = ev.event;
		if (evType === 'appt-schedule') {
			return this.#mapApptScheduled(ev);
		} else if ((evType === 'web.webpagedetails.pageViews' || evType === 'route-change') && ev.web?.webPageDetails?.viewName === 'drive-results') {
			return this.#mapApptSearch(ev);
		} else if(evType === 'rcb-comm-pref-update') {
			return this.#mapCommUpdate(ev);
		} else if ((evType === 'web.webpagedetails.pageViews' || evType === 'route-change') && ev.web?.webPageDetails?.viewName && document.location.pathname.includes('create-account-confirmation')) {
			ev.web.webPageDetails.viewName ='create-account-confirmation';
		} else if((evType === 'web.webpagedetails.pageViews' || evType === 'route-change') && ev.web?.webPageDetails?.viewName && document.location.pathname.includes('temporary-password-login')) {
			ev.web.webPageDetails.viewName ='temporary-password-login';
		} else if((evType === 'web.webpagedetails.pageViews' || evType === 'route-change') && ev.web?.webPageDetails?.viewName && document.location.pathname.includes('temporary-password-reset')) {
			ev.web.webPageDetails.viewName ='temporary-password-reset';
		} else if((evType === 'web.webpagedetails.pageViews' || evType === 'route-change') && ev.web?.webPageDetails?.viewName && document.location.pathname.includes('find-drive-advanced')) {
			ev.web.webPageDetails.viewName ='find-drive-advanced';
		} else if((evType === 'web.webpagedetails.pageViews' || evType === 'route-change') && ev.web?.webPageDetails?.viewName && document.location.pathname.includes('find-drive')) {
			ev.web.webPageDetails.viewName ='find-drive';
		}

		return ev;

	}

	// TODO: This should not be necessary. Add to SPA.
	#mapApptScheduled(detail) {
		const retDetail = detail;
		if (!detail._redcross || !detail._redcross.bloodAppointment) {
			return retDetail;
		}

		retDetail._redcross.bloodAppointment.driveType = this._dataStore.getVar('driveType');

		if (detail._redcross.bloodAppointment.scheduleMethod === 'one-click' && !detail._redcross.bloodAppointment.procedureType) {
			const procType = this._dataStore.getVar('procedureType');
			if (procType.includes("blood")) {
				retDetail._redcross.bloodAppointment.procedureType = 'blood';
			} else if (procType.includes('platelets')) {
				retDetail._redcross.bloodAppointment.procedureType = 'platelets';
			} else if (procType.includes('power red')) {
				retDetail._redcross.bloodAppointment.procedureType = 'power-red';
			} else if (procType.includes('ab plasma')) {
				retDetail._redcross.bloodAppointment.procedureType = 'plasma';
			}
		}

		return retDetail;


	}
// TODO: This should not be necessary. Add to AEM code base.
	#mapApptSearch(detail) {
		const retDetail = detail;

		if(localStorage.getItem('blooddrivesearches') || localStorage.getItem('location-map-button')) {
			
			retDetail._experience ??= {analytics: { event1to100: {event71: {}}, event201to300: {event216: {}}}};
			retDetail._experience.analytics ??= { event1to100: {event71: {}}, event201to300: {event216: {}}};
			retDetail._experience.analytics.event1to100 ??= {event71: {}};
			retDetail._experience.analytics.event201to300  ??= {event216:{}};
			retDetail._experience.analytics.event1to100.event71.value = 1;
			retDetail._experience.analytics.event201to300.event216.value = 1;


			retDetail.web ??= {webPageDetails: {pageViews: {value:1}}}
			retDetail.web.webPageDetails ??= {pageViews: {value: 1}};
			retDetail.web.webPageDetails.pageViews ??= {value : 1 };
			retDetail.web.webPageDetails.pageViews.value = 1;

			retDetail._redcross ??= { bloodSearch: {} };
			retDetail._redcross.bloodSearch ??= {};

			const sourceString = localStorage.getItem('location-map-button') ? 'local' : localStorage.getItem('blooddrivesearches');
			retDetail._redcross.bloodSearch.location =  sourceString;
			retDetail._redcross.bloodSearch.location =  sourceString;

			const urlParams = new URLSearchParams(window.location.search);
			retDetail._redcross.bloodSearch.term = urlParams.get('zipSponsor');

			localStorage.removeItem('location-map-button');
			localStorage.removeItem('blooddrivesearches');

			return retDetail;
		} else {
			return retDetail;
		}
	}
/* TODO: This should not be necessary. Update AEM code base to 
explicitly set the property to false if user does not opt-in (currently the 'opt-in' property is absent/undefined ) */
	#mapCommUpdate(ev) {
		const retDetail = ev;
		retDetail._redcross ??=  {user: {smsOptIn: false, emailOptIn: false}};
		retDetail._redcross.user ??=  {smsOptIn: false, emailOptIn: false};
		retDetail._redcross.user.smsOptIn ??= false;
		retDetail._redcross.user.emailOptIn ??= false;
		return retDetail;
	}

}

