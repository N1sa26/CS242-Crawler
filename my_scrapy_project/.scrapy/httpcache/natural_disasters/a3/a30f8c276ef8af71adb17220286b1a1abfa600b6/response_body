});</script><script>_satellite["_runScript2"](function(event, target, Promise) {
class CampaignUtils {

	getCampaignData() {
	const urlParams = new URLSearchParams(window.location.search);
	if (!urlParams) { return undefined };

	
	const internalPromotions = this.#getInternalCampaigns();

	const retObj = { _redcross: {
		marketingCampaign: {
		}
	}};

	this.#setCoreTracking(retObj, urlParams);
	this.#setAdobeCampaignTracking(retObj, urlParams);

	if(internalPromotions) {retObj._redcross.marketingCampaign.internalPromotions =internalPromotions;}
	
	// Unable to build tracking code name from core tracking; look at UTM
	if(!retObj?.marketing?.trackingCode) {
		const res = this.#getUTMCName(urlParams);
		if(res) {
			retObj.marketing = {trackingCode: res};
			retObj._redcross.marketingCampaign.trackingCode = res;
		};
	}

 	if(!retObj.marketing && !internalPromotions) {
		return undefined;
	}

	return retObj;

}



#setCoreTracking(arcXDMObj, urlParams) {
	arcXDMObj._redcross ??= {marketingCampaign:{}};
	arcXDMObj._redcross.marketingCampaign ??= {};

	const cid = urlParams.get('cid');
	const med = urlParams.get('med');
	const desc = urlParams.get('campdesc');
	const source = urlParams.get('source');
	const sourceCode = urlParams.get('scode');
	const subCode = urlParams.get('subcode');

	if(med) {arcXDMObj._redcross.marketingCampaign.campaignChannel = med;}
	if(desc) {arcXDMObj._redcross.marketingCampaign.campaignDesc = desc;}
	if(cid) {arcXDMObj._redcross.marketingCampaign.campaignId =cid;}
	if(source) {arcXDMObj._redcross.marketingCampaign.campaignSource = source;}
	if(sourceCode) {arcXDMObj._redcross.marketingCampaign.scode =sourceCode;}
	if(subCode) {arcXDMObj._redcross.marketingCampaign.subCode =subCode;}
	
	if(cid) {
		const trackingCode = this.#getCName(arcXDMObj);
		arcXDMObj.marketing ={trackingCode: trackingCode };
		arcXDMObj._redcross.marketingCampaign.trackingCode =trackingCode;
	}
	return arcXDMObj;
}

#setAdobeCampaignTracking(arcXDMObj, urlParams) {
	const bid =  urlParams.get('bid');
	const did = urlParams.get('did');
	const subj = urlParams.get('subj');
	const prod = urlParams.get('prod');
	const delvid = urlParams.get('delvId');

		
	if(bid) {arcXDMObj._redcross.marketingCampaign.emacBroadloadId =bid;}
	if(did) {arcXDMObj._redcross.marketingCampaign.emacCampaignId =did;}
	if(subj) {arcXDMObj._redcross.marketingCampaign.emacSubj =subj;}
	if(prod) {arcXDMObj._redcross.marketingCampaign.emacProd = prod;}
	if(delvid) {arcXDMObj._redcross.marketingCampaign.emacDelvId = delvid;}

	return arcXDMObj;
}


#getUTMCName(urlParams) {
	if(urlParams.get('utm_source')  ||  urlParams.get('utm_campaign') ||  urlParams.get('utm_medium') ||  urlParams.get('utm_term') ||  urlParams.get('utm_content') ) {
		const utmSource = urlParams.get('utm_source') ?  urlParams.get('utm_source') : 'notset'
		const utmCampaign = urlParams.get('utm_campaign') ?  urlParams.get('utm_campaign')  : 'notset';
		const utmMedium = urlParams.get('utm_medium') ? urlParams.get('utm_medium') : 'notset';
		const utmTerm = urlParams.get('utm_term') ? urlParams.get('utm_term') : 'notset';
		const utmContent = urlParams.get('utm_content') ? urlParams.get('utm_content') : 'notset';
		return `${utmMedium}:${utmSource}:${utmCampaign}:${utmTerm}:${utmContent}`;
	} 
	return undefined;
}


#getCName(arcXDMObj) {
	if(arcXDMObj?._redcross?.marketingCampaign?.campaignId) {
		const marketingCampaign =arcXDMObj._redcross.marketingCampaign;
		let cName = `cid=${marketingCampaign.campaignId}`;
		cName +=  marketingCampaign.campaignChannel ? ':med=' +  marketingCampaign.campaignChannel : '';
		cName += marketingCampaign.campaignSource ? ':source=' + marketingCampaign.campaignSource : '';
		cName += marketingCampaign.scode ? ':scode=' + marketingCampaign.scode : '';
		cName += marketingCampaign.campaignDesc ? ':campdesc=' + marketingCampaign.campaignDesc : '';
		cName += marketingCampaign.subCode ? ':subcode=' + marketingCampaign.subCode : '';
		return cName;

	} 
	return undefined;
	
}
 #getInternalCampaigns() {
	const urlParams = new URLSearchParams(window.location.search);
	if (!urlParams) { return undefined };

	const iCid = urlParams.get('icid');
	const iMed = urlParams.get('imed');
	const iSource = urlParams.get('isource');
	const iSourceCode = urlParams.get('iscode');
	const iCamp = urlParams.get('icampdesc');
	const iSubCode = urlParams.get('isubcode');

	if(iCid && iMed && iSource) {
		let cString 	= 'icid=' + iCid + ':imed=' + iMed + ':isource=' + iSource;
		cString += iSourceCode ? ':iscode=' + iSourceCode : '';
		cString += iCamp ? ':icampdesc=' + iCamp : '';
		cString += iSubCode ? ':isubcode=' + iSubCode : '';
		return cString;
		}	
		return undefined;
}

}


if (!window.arcCampaignUtils) { window.arcCampaignUtils = new CampaignUtils() }
});</script><script>_satellite["_runScript3"](function(event, target, Promise) {

async function getGeoPermission() {
	try {
		const permissionStatus = await navigator?.permissions?.query({ name: 'geolocation' })
		const permState = permissionStatus?.state;
		return permState || 'denied';
	} catch (er) {
		return 'denied';
	}
}


getGeoPermission().then(res => {
	const data = {
		event: 'geolocation-change',
		_redcross: { user: { geoLocationPermission: res } }
	}
	if (document.body) {
		document.body.dispatchEvent(new CustomEvent('arc-web-event', { detail: data }));
	}
});
