    const RAND = Math.random().toString(36).substring(2);

    if (!(_satellite.cookie.get(ID_CHECKED_COOKIE) || _satellite.cookie.get(ID_COOKIE))) {
        getARCID()
            .then(res => {
                _satellite.cookie.set(ID_COOKIE, res, {
                    expires: 365
                });
            })
            .catch(er => createCheckedCookie());

    }

    async function getARCID() {
        const response = await fetch(FPID_ENDPOINT + '?rand=' + RAND);
        const json = await response.json();
        return json.id;
    }
  
}

    // Sets a cookie to indicate that ID endpoint should not be queried again; lifetime = 1 hour.
    function createCheckedCookie() {
        _satellite.cookie.set(ID_CHECKED_COOKIE, {
            expires: (1 / 24)
        });
    }
