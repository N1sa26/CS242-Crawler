</script><script>_satellite["_runScript9"](function(event, target, Promise) {

const btn = document.querySelector('[href^="https://americanredcross.donordrive.com"]');
const DEF_PRG_ID = '153-100';

if (btn) {
	const href = btn.getAttribute('href');
	const pID = getProgramId(href);
	injectDonorDrive(pID);

}

function injectDonorDrive(programId) {
	const scr = document.createElement('script');
	scr.src = 'https://americanredcross.donordrive.com/resources/js/embed-donate.js?v=202408280117';
	scr.setAttribute('data-program-id', programId);
	scr.setAttribute('async', true);
	const footer = document.querySelector('footer');
	if (footer) {
		footer.insertAdjacentElement('afterbegin', scr);
	} else {
		document.body.appendChild(scr);
	}
}

function getProgramId(href) {
	if (!href) { return DEF_PRG_ID; }
	const url = new URL(href);
	const sParams = url?.searchParams;
	return sParams.get('programID') || DEF_PRG_ID;
}



});</script>
