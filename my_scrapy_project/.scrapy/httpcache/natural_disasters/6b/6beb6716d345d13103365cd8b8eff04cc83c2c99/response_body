      });
      try {
        observer.observe({ type: 'mark', buffered: true });
        observer.observe({ type: 'measure', buffered: true });
      } catch (error) {
        console.error('Buffered PerformanceObserver Not Supported', error)
      }
    }
  })();
  (()=> {
    let highestInp = 0;
    const observer = new PerformanceObserver((list, obs, options) => {
      for (let entry of list.getEntries()) {
        if (!entry.interactionId) continue;
        const duration = Math.round(entry.duration);
        if (duration <= highestInp) continue;
        highestInp = duration;
        const startTime = Math.round(entry.startTime);

        let label = entry.target?.ariaLabel?.replaceAll(" ", "_");
        label = label ? label : entry.target?.localName;
        let selector = entry.target?.classList?.value?.replaceAll(" ", "_");
        if (!selector) {
          selector = entry.target?.parentElement?.ariaLabel?.replaceAll(" ", "_") || entry.target?.href?.baseVal;
        }
        if (!label || !selector) {
          // likely video
          label = label ? label : window.utag_data?.user_type;
          const hasVideo = document.querySelector('[data-type="video"]')
          selector = selector ? selector : hasVideo ? 'video' : 'no-video';
        }
        const target = label + ':' + selector;
        if (window.newrelic) {
          if (window.requestIdleCallback) {
            requestIdleCallback(() => {
              newrelic.setCustomAttribute('inpSlowEventTarget', target);
              newrelic.setCustomAttribute('inpSlowEventStartTime', startTime);
            });
          } else {
            newrelic.setCustomAttribute('inpSlowEventTarget', target);
            newrelic.setCustomAttribute('inpSlowEventStartTime', startTime);
          }
        }
      }
    });
    try {
      observer.observe({
        type: 'event',
        durationThreshold: 200, // good INP is under 200ms
        buffered: true
      });
    } catch (error) {
      console.error('PerformanceObserver failed', error)
    }
  })()
</script><iframe name="__tcfapiLocator" style="display: none;"></iframe><iframe name="__uspapiLocator" style="display: none;"></iframe><iframe name="__gppLocator" style="display: none;"></iframe><script src="https://accounts.google.com/gsi/client" async="true" defer="true" data-nscript="afterInteractive"></script><script src="https://news.google.com/swg/js/v1/swg.js" id="swg" subscriptions-control="manual" data-nscript="afterInteractive"></script><script src="https://news.google.com/swg/js/v1/swg-gaa.js" id="swg-gaa" data-nscript="afterInteractive"></script><script src="https://www.marketwatch.com/asset/dj-ufc/loaders/marketwatch.js" async="true" data-nscript="afterInteractive"></script><script src="https://cdn.p-n.io/pushly-sdk.min.js?domain_key=dDO9rW5BJjscrJF1odg2XLzg1OMtCyVUWN1H" async="true" data-nscript="afterInteractive"></script><next-route-announcer><p aria-live="assertive" id="__next-route-announcer__" role="alert" style="border: 0px; clip: rect(0px, 0px, 0px, 0px); height: 1px; margin: -1px; overflow: hidden; padding: 0px; position: absolute; top: 0px; width: 1px; white-space: nowrap; overflow-wrap: normal;"></p></next-route-announcer><div class="nk-layer-organizer"></div><iframe marginwidth="0" marginheight="0" scrolling="no" frameborder="0" id="15b17086c9ceb7" width="0" height="0" src="about:blank" name="__pb_locator__" style="display: none; height: 0px; width: 0px; border: 0px;"></iframe><script src="https://sts3.wsj.net/iweb/static_html_files/cxense-candy.js" data-nscript="afterInteractive"></script><iframe src="https://news.google.com/swg/ui/v1/serviceiframe?_=1739252588584&amp;publicationId=marketwatch.com" style="opacity: 0 !important; position: absolute !important; top: -10px !important; left: -10px !important; height: 1px !important; width: 1px !important;"></iframe><script type="module" src="https://www.marketwatch.com/asset/dj-ufc/v1.34.0/dj-ufc.esm.js"></script><script nomodule="true" src="https://www.marketwatch.com/asset/dj-ufc/v1.34.0/dj-uf