  handleLogInBtnClick();

  function handleLogInBtnClick() {
    var $logInBtn = document.querySelector('#cx-callout-tile-wrapper .link.log--in--btn');
    if (!$logInBtn) { return; }

    $logInBtn.addEventListener('click', function() {
      var currentUrl = encodeURIComponent(window.location.href);
      $logInBtn.href  += ($logInBtn.href.indexOf('?') !== -1 ? '&' : '?') + 'target=' + currentUrl;
    });
  }
});

