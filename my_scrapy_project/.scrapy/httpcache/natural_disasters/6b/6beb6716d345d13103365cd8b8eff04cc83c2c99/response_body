  (function() {
    window.googletag = window.googletag || { cmd: [] };

    const getCurrentUser = async(e,t="")=>{let r={cache:"no-cache",credentials:"include",headers:{"x-original-host":window.location.host,"x-original-referrer":window.document.referrer,"x-original-url":window.location.pathname+window.location.search+window.location.hash},timeout:3e3};t&&""!=t&&"null"!=t&&(r.headers["X-ENCRYPTED-DOCUMENT-KEY"]=t);try{let t=await fetch(`${e}?legacy=false`,r),n=await t.json();return n?.type&&(n.status="user_logged_in"),await new Promise(e=>{setTimeout(e,0)}),window?.entitlementsAsyncData?.resolve&&window?.entitlementsAsyncData?.resolve(n),n}catch(e){return window?.entitlementsAsyncData?.reject(e),{}}};

    function deferredPromise() {
      let _resolve;
      let _reject;
      const promise = new Promise((resolve, reject) => {
        _resolve = resolve;
        _reject = reject;
      });
      promise.resolve = _resolve;
      promise.reject = _reject;
      return promise;
    }

    /**
     * Make a request to get the entitlements of the current user
     */
    function requestEntitlements() {
      performance.mark('ads-request-entitlements');
      window.entitlementsAsyncData = deferredPromise();
      // Get entitilements (user data)
      const queryParams = new URLSearchParams(window.location.search);
      const hasAdPageType = false;
      const isSnippetView = 'true' === 'true';
      window.__isSnippetView = isSnippetView;

      const targeting = {
        alert: "undefined",
        msrc: queryParams.get('mod') || '',
        application_stack: "gcn",
        breakpoint: "at16units",
        page: "article",
      };

      if (hasAdPageType) {
        targeting.pagetype = 'undefined';
      }

      if(isSnippetView) {
        const userTargeting = {
          circ: 'nonsubscriber',
          // AdTech renamed circ to usertype in data layer
          usertype: 'nonsubscriber'
        };

        window.utag_data.profiles = '';
        window.utag_data.user_exp = 'default';
        window.utag_data.user_ref = '';
        window.utag_data.user_tags = '';
        window.utag_data.user_type = 'nonsubscriber';
        window.utag_data.article_template = 'preview';

        try {
          window.__ace('dataLayer', 'setData',[{...targeting, ...userTargeting}]);
        } catch(e) {}
      }

      // saving entitlements fetch promise in global object
      const entitlementsUrl = 'https://www.marketwatch.com/client';
      const encryptedDocumentKey = 'pc8jsdwDrgGBUt7mrkbJu15JUkI+4rQ6Mg07g/FKqjq4ijgRKbvzDoeZNpPSF2A1mRWWpShlud91T1Uy08U8LjuD9ogL7o8SYBwkFNhx5qnpuqSHzHDjRuDqskrUhYOOyl0GHOfiIPq46VVdwgukNd3blalrlpNrYYIEQ5OqAL8VJHuAH36b/l00kiYXx+3986RIxqJS9y9pvbXFfBb9QDXrkIs/Qx+vR5mkKu+axSwSPzl3GiRkr1Hk3g+I7g==';
      if (true) {
        getCurrentUser(entitlementsUrl, encryptedDocumentKey, { cache: false });
      }

      window.entitlementsAsyncData
        .then((retrievedUser = {}) => {
          performance.mark('ads-receive-entitlements');

          if (retrievedUser.redirect) {
            window.location.href = retrievedUser.redirect;
            return;
          }

          const {
            companyDetails: { company_codes: companyCodes, name: companyName = '' } = {},
            documentKey,
            execEdition,
            experience,
            facebookSub,
            tags,
            type,
            vxid,
            paywall,
            permissions: { wsjPausedSub } = {},
            emailHashes,
            profiles,
          } = retrievedUser || {};

          // Set emailHashes property in DL and call Liveramp via GMS
          if(emailHashes){
            window.__ace('dataLayer', 'setData',[{emailHashes}]);
            window.__ace('liveramp','setEmailHashes',[emailHashes]);
          }

          window.utag_data.cx_shield = paywall;

          if(!isSnippetView) {
            const userTargeting = {
              circ: type,
              // AdTech renamed circ to usertype in data layer
              usertype: type
            };

            window.utag_data.profiles = profiles || '';
            window.utag_data.user_exp = experience || '';
            window.utag_data.user_ref = (vxid && `V2-${vxid}`) || '';
            window.utag_data.user_tags = (tags && tags.join) ? tags.join(',') : '';
            window.utag_data.user_type = type || '';
            window.utag_data.article_template = !documentKey ? 'preview' : 'full';

            if (wsjPausedSub) {
              window.utag_data.sub_paused = 'true';
            }

            if (facebookSub) {
              window.utag_data.fbSub = 'true';
            }

            try {
              window.__ace('dataLayer', 'setData',[{...targeting, ...userTargeting}]);
            } catch(e) {}
          }
        })
        .catch((error) => console.error(error));
    }

    // First, get the entitlements
    requestEntitlements();

    // dump this here to limit the script references needed
    // handle events from GPT
    googletag.cmd.push(function() {
      googletag.pubads().addEventListener('slotRenderEnded', function (event) {
        const el = document.getElementById(event.slot.getSlotElementId());
        const wrapper = el ? el.closest('.ad-slot') : null;
        if (wrapper) {
          wrapper.dispatchEvent(new CustomEvent('slotRendered', { detail: { origEvent: event, el } }));
        }
      });
    });
  })();
