  const handleNavHatClick = () => {
    const wrapper = document.querySelector('#cx-nav-hat-wrapper');
    const [nav] = document.getElementsByTagName('nav-hat');

    if (!wrapper || !nav) {
      return;
    }
    
	const isWSJPlus = nav.getAttribute('variation') === 'wsjplus';
    const modCode = nav.modCode;
    const ctaText = wrapper.dataset.ctaText;
    const url = wrapper.dataset.url;
    const clickUrl = wrapper.dataset.clickUrl;
    const widgetId = '5dca299f5bdc74d5edfb93b30e9ab844020f8475';
    const campaignId = wrapper.dataset.campaignId;
    
    let targetUrl;
    
    if (!url || !clickUrl || isWSJPlus) {
      return;
    }
 
    if (ctaText) {
      nav.ctaText = ctaText;
    }
	
    targetUrl = cxenseOffers.addQueryStringToUrl(clickUrl, { mod: modCode });
    targetUrl =  cxenseOffers.addModuleInfoToUrl(targetUrl, widgetId , campaignId);
    nav.showCta = true;
    nav.ctaUrl = targetUrl;
  };

  handleNavHatClick();
});
