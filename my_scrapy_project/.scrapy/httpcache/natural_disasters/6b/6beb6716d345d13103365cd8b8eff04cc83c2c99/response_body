  url = cxenseOffers.addModuleInfoToUrl(url, widgetId , campaign);
  window.location.href = url;
  utag.link({'event_name': 'subscribe_link', 'subscribe_location': 'pw_header'});

}

window.requestAnimationFrame(function () {
  handleNonsubPopupMouseOver();
  handleNonsubPopupMouseLeave();
  handleNonsubPopupClick();

  function handleNonsubPopupMouseOver() {

    var $wrapper = document.querySelector('#cx-popout-wrapper');
    if (!$wrapper) { return; }

    var $popout = $wrapper.querySelector('.cx-popout');
    if (!$popout) { return; }

    $wrapper.addEventListener('mouseover', function (event) {
      event.stopPropagation();
      $popout.classList.remove('hide');
      $popout.classList.add('show');
      $popout.classList.remove('fade-out');
      $popout.classList.add('fade-in');
    });
  }

  function handleNonsubPopupMouseLeave() {

    var $wrapper = document.querySelector('#cx-popout-wrapper');
    if (!$wrapper) { return; }

    var $popout = $wrapper.querySelector('.cx-popout');
    if (!$popout) { return; }

    $wrapper.addEventListener('mouseleave', function (event) {
      event.stopPropagation();
      $popout.classList.remove('fade-in');
      $popout.classList.add('fade-out');

      setTimeout(function () {
        $popout.classList.remove('show');
        $popout.classList.add('hide');
      }, 100);
    });
  }

  function handleNonsubPopupClick() {
    var $module = document.querySelector('#cx-popout');
    if (!$module) {
      return;
    }

    $module.addEventListener('click', function (event) {
      event.stopPropagation();
    });
  }

});</script><style type="text/css">#cx-marketing-tile,
