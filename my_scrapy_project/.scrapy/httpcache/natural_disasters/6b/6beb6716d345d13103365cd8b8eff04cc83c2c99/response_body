  const handleCloseButtonClick = () => {
    const overlay = document.querySelector("#cx-article-cover-overlay");
    if (!overlay) {
      return;
    }

    const closeButton = overlay.querySelector(".close-btn");
    if (!closeButton) {
      return;
    }

    closeButton.addEventListener("click", (event) => {
      event.stopPropagation();
      cxenseOffers.fadeOut(overlay, 400, () => {
        overlay.remove();
      });
    });
  };

  handleCloseButtonClick();
});

function cxHandleArticleCoverClick(item){
  const widgetId = '5a6132e17936ff4f8115bd8104cdd66c356a0313';
  const { campaign } = item;
  let { url } = item;

  // Get Automated Pricing URL if Automated Pricing is enabled.
  url = item['Automated Pricing'] === 'true' ? cxenseOffers.getAutomatedPricingUrl(url) : url;
  // Add Cxense Module Info for Adobe Tracking
  url = cxenseOffers.addModuleInfoToUrl(url, widgetId , campaign);
  console.log('targetUrl: ', url);
  window.location.href =  url;
  utag.link({'event_name':'subscribe_link', 'subscribe_location': 'pw_articlecover'});
}</script><style type="text/css">#targetElement a { /* GUI previews */
