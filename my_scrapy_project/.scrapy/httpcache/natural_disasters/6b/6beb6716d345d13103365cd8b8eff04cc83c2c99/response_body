  const widgetId = '48e9c719e21a2f9226b1487fba341059455dd976';
  const { campaign } = item;
  let { url } = item;

  // Get Automated Pricing URL if Automated Pricing is enabled.
  url = item['Automated Pricing'] === 'true' ? cxenseOffers.getAutomatedPricingUrl(url) : url;
  // Add Cxense Module Info for Adobe Tracking
  url = cxenseOffers.addModuleInfoToUrl(url, widgetId , campaign);
  window.location.href = url;
  utag.link({'event_name': 'subscribe_link', 'subscribe_location': 'pw_sidemenu'});
