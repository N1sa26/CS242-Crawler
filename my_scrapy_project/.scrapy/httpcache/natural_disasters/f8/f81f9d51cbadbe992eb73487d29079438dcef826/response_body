<script>
console.log('Fire Now');
var tagText = $('.LedePR-readingTime').next().find('a').text();
//$('.LedePR-readingTime').next('div').html(`<p class="LedePR-topics">${tagText}</p>`);

//fix for the issue with assets in the in page nav adding .html
$(document).ready(function () {
        
	$('.in-page-navigation-2-0').find('a').each(function () {
try{
		if($(this).attr('href').indexOf('.pdf.html')>0) {
			let href = $(this).attr('href')
			$(this).attr('href', href.replace('.pdf.html', '.pdf'))
		}
 } catch (error) {
    console.log(error);
 }
try{
		if($(this).attr('href').indexOf('.xlsx.html')>0) {
			let href = $(this).attr('href')
			$(this).attr('href', href.replace('.xlsx.html', '.xlsx'))
		}
 } catch (error) {
       console.log(error);
}
	})

});

</script> </div>
	

