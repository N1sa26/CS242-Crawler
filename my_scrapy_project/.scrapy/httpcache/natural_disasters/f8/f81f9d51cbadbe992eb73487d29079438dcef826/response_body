<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
<script>
    const swiper = new Swiper('.swiper', {
        slidesPerGroup: 1,
        slidesPerView: 1,
        autoHeight: true,
        pagination: {
            el: ".swiper-pagination",
            clickable: true,
        },
        navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
        },
    });

    function checkViewportAndRemoveSlides() {
        if (window.innerWidth < 768) {
            if (swiper.slides.length == 3) {
                swiper.removeAllSlides();
                swiper.addSlide(1, mobileslides);
                swiper.update();
            }
        } else if (window.innerWidth >= 768 && swiper.slides.length == 6) {
            swiper.removeAllSlides();

            swiper.addSlide(1, desktopslides);
            swiper.update();
        }
    }

    function debounce(func, wait) {
        let timeout;
        return function() {
            const context = this;
            const args = arguments;
            clearTimeout(timeout);
            timeout = setTimeout(() => func.apply(context, args), wait);
        };
    }

    swiper.on('slidesUpdated', function() {
        checkViewportAndRemoveSlides();
    });

    window.addEventListener('resize', debounce(checkViewportAndRemoveSlides, 200));

    document.querySelectorAll('.hiprt__dot').forEach(function(dot) {
        dot.addEventListener('click', function() {
            let index = this.getAttribute('data-slide');
            document.querySelectorAll('.hiprt__dot').forEach(function(dot) {
                dot.classList.remove('hiprt__dot--active');
            });

            this.classList.add('hiprt__dot--active');
            swiper.slideTo(index, 300);
            this.blur();
        });
    });

    document.querySelector('.swiper-button-next').addEventListener('click', function() {
        this.blur();
    });

    document.querySelector('.swiper-button-prev').addEventListener('click', function() {
        this.blur();
    });

    swiper.on('activeIndexChange', function() {
        document.querySelectorAll('.hiprt__dot').forEach(function(dot) {
            dot.classList.remove('hiprt__dot--active');
        });

        let targetDot = document.querySelector(`.hiprt__dot[data-slide='${swiper.activeIndex}']`);
        if (targetDot) {
            targetDot.classList.add('hiprt__dot--active');
        }

    });


    var desktopslides = [`<div class="swiper-slide">
