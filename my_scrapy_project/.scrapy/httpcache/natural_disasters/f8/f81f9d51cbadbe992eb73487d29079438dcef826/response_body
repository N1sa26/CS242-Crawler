	<!-- Raw HTML -->
	
		<div class="rte-styles margin-bottom-0">
			<!-- hide times on search results for events -->
<script>
function removeTextSearchPage() {
	 const reg1 = /\w\w\w\s\d\W\s\d\d\d\d/;
	 const reg2 = /\w\w\w\s\d\d\W\s\d\d\d\d/;
	let originalText = "";
	const collection = document.getElementsByClassName("cs-events-date");
	for (let i = 0; i < collection.length; i++) {
	originalText = collection[i].innerHTML;
	if (originalText) {
		const newStr = originalText.match(reg2);
		if (newStr != null) {
                 document.getElementsByClassName("cs-events-date")[i].innerHTML = newStr;
               }
		else  {
                 const newStr = originalText.match(reg1);
			 if (newStr) {
				document.getElementsByClassName("cs-events-date")[i].innerHTML = newStr;
			      }
	              }
		}
     }
};

$(document).ready(function () {
if (window.location.href.indexOf('tab=events.html') > 0) {
 setTimeout(function () {
	 removeTextSearchPage() 
},7000)
} else {
 setTimeout(function () {
	 removeTextSearchPage() 
},4000)
}
}) ;
// click on events tab
$('.federated-tabs').find('.tab[data-tab="events"]').on('click', function () {
    setTimeout(function () {
     removeTextSearchPage()
       }, 500) 
});
// click on see all link
$('.js-view-all[data-tab="events"]').on('click', function () {
     setTimeout(function () {
	 removeTextSearchPage() 
},2000)
});
</script> </div>
	

	<!-- HTML Form -->

	

</div>


    
    
