<script>
$(document).ready(function () {
if ($('body').hasClass( "corporate-media-library-template" )) {
      // message portion
       var newDiv = $("<div class='media-library-temp-user-message'><span>Looking for a specific asset? Enter a keyword to refine your results.</span></div>");
        
       newDiv.insertBefore("#corporate-search-bar");
   //      $('.imageContainer').find('.see-all').on('click', function () {
   //           hidePagination();
   //       })

  //      $('.tab.swiper-slide[data-tab="images"]').on('click', function () {
  //            hidePagination();
  //      })
      
}

function hidePagination() {

       $('.search-page-num').each(function () {
             
            let dataid = parseInt($(this).attr('id'))
            if (dataid >= 4) {
                   $(this).css({'display': 'none'})
             }
          })
}
})
</script>


