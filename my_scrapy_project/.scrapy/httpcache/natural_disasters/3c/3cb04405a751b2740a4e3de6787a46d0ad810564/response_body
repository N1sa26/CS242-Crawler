                <span aria-label="Take Action">Take Action</span>
                <span class="hidden lg:group-hover:block lg:group-focus:block h-1 w-full absolute bottom-0 bg-un-primary-light"></span>
              </a>
                          </li>
                                  <li class="py-5 ps-7.5 pe-15 lg:ps-0 lg:pe-0 lg:p-0 cursor-pointer group lg:relative">
                            <a class="font-bold lg:font-normal lg:group-hover:font-bold lg:group-focus:font-bold text-xl lg:text-lg justify-between lg:justify-center lg:py-4 lg:px-6 group flex" href="/en/stories" data-once="submenu-reveal">
                <span aria-label="Stories">Stories</span>
                <span class="hidden lg:group-hover:block lg:group-focus:block h-1 w-full absolute bottom-0 bg-un-primary-light"></span>
              </a>
                          </li>
                                  <li class="py-5 ps-7.5 pe-15 lg:ps-0 lg:pe-0 lg:p-0 cursor-pointer group lg:relative has-children">
                            <a class="font-bold lg:font-normal lg:group-hover:font-bold lg:group-focus:font-bold text-xl lg:text-lg justify-between lg:justify-center lg:py-4 lg:px-6 group flex" href="#" data-once="submenu-reveal">
                <span aria-label="Resources">Resources</span>
                <span class="hidden lg:group-hover:block lg:group-focus:block h-1 w-full absolute bottom-0 bg-un-primary-light"></span>
              </a>
                                              <ul class="border-t border-un-gray-light flex-col divide-y divide-un-gray-light mt-4 lg:mt-0 hidden lg:hidden lg:group-hover:flex lg:group-focus:flex lg:absolute lg:top-full z-50 lg:bg-un-primary-main lg:w-80 lg:py-4 lg:px-6 lg:text-white lg:shadow-md">
