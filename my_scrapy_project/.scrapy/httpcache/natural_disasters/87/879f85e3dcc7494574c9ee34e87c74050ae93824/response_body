    .map-iframe-overlay {
      position: absolute;
      background: rgba(0, 0, 0, 0.3);
      color: #fff;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 1.2em;
      cursor: pointer;
      font-family: Georgia, serif;
      transition: background 0.2s ease;
    }
    .map-iframe-overlay:hover {
      background: rgba(0, 0, 0, 0.4);
