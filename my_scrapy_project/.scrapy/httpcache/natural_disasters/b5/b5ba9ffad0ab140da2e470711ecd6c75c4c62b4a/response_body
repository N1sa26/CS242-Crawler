<script>
var promoOpenedFromModal = false;
$(function(){
    $('#service-promo-open').on('submit',function(){
    
        if($('#service-promo-open').find('input[name=promo]').val()!=""){
            $('#service-promo-modal').find('input[name=promo]').val($('#service-promo-open').find('input[name=promo]').val());
            $('#service-promo-modal').find('.promo-loading').show();
    
            if($('#subscription-modal').is(':visible')){
                $('#subscription-modal').modal('toggle');
                promoOpenedFromModal = true;
                setTimeout(function () {
                    $('#service-promo-modal').find('.modal').modal('toggle').submit();
                },500);
            }else{
                $('#service-promo-modal').find('.modal').modal('toggle').submit();
            }
       }
    
        return false;
    });


    var searchParams = new URLSearchParams(document.location.search),
        promoOpen = document.getElementById('service-promo-open'),
        promoModal = document.getElementById('service-promo-modal'),
        promoForm = null,
        promoEntry = null,
        promoCode = null,
        promoEl = null;

    if(promoModal){
        promoEntry = promoModal.querySelector('input[name=promo]');
        promoCode = searchParams.get('promoCode');
        promoEl = $(promoModal);
    
        if(promoEntry && promoCode){ 
            promoEntry.value = promoCode;

            setTimeout(function () {

                __tnt.subscription.promos.fetch(promoEl, promoCode, false, function(){
                    $('#subscription-modal').modal('hide');
                
                    setTimeout(function () {
                        $(promoModal).find('.modal').modal('show');
                    }, 1000);
                });

            },500);
        }
    }
    
});
</script>

