                if (referrer && socialReferrers.indexOf(referrer) > -1 ) {
                    sMode_1928016 = 'block-contextual';
                    bDmpTrack = false;
                    iMaxAttempt = 1;
                } 
            
            function getRecommendations_1928016(aAudiences) {
                iAttempt ++;
                //__tnt.log('\nRunning getRecommendations. Attempt ' +  iAttempt + ' of ' + iMaxAttempt);
                var oEl = document.getElementById('tncms-recommendations-1928016' );
