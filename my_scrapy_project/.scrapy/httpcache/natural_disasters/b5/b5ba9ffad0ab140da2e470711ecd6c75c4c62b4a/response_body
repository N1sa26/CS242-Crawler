                            iMax = 4;
                        for(var nIdx = 0; (nIdx < oResponse.assets.length && iCount < iMax); nIdx++) { 
                            if ( oResponse.assets[nIdx].content.includes('inline-summary') && !(oResponse.assets[nIdx].content.includes('f4778ffe-e349-11ef-9e82-37d67f894376'))  ) {
                                iCount++;
                                sHTML += '<div class=" col-sm-3">';
