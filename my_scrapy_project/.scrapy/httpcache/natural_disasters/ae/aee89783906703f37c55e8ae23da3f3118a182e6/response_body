<script>
const lowercaseDoNotSellLinkText = 'do not sell or share my personal information (ca)';
let cookieConsentToggleState = 'na';
let formattedSiteName = window.location?.hostname?.toUpperCase().replace('.COM', '');
if (formattedSiteName.includes('P-MICRO')) {
  formattedSiteName = window.location?.pathname?.split('/')[1]?.toUpperCase();
}

const trackCookieConsentClicks = obj => {
  if (window.dataLayer && Array.isArray(window.dataLayer)) {
    window.dataLayer.push(obj);
  }
};

const checkIfDoNotSellLink = el => {
  if (!el) return false;
  return (
    el.innerText?.toLowerCase().indexOf(lowercaseDoNotSellLinkText) === 0 ||
    el.innerHTML?.toLowerCase().indexOf(lowercaseDoNotSellLinkText) === 0
  );
};

const removeDoNotSellRedirect = () => {
  const aTags = document.getElementsByTagName('a');
  for (let i = 0; i < aTags.length; i++) {
    const a = aTags[i];
    const modalLink = checkIfDoNotSellLink(a);
    if (modalLink) {
      a.onclick = e => {
        e.preventDefault();
        const otGeoState = window.OneTrust?.getGeolocationData()?.state?.toLowerCase() || 'na';
        trackCookieConsentClicks({
          event: 'send-VPV',
          'vpv-name': `/vpv/de/modal/cookie-settings/load/do-not-sell-or-share-my-personal-information/${
            formattedSiteName || 'na'
          }/na/${otGeoState}`,
        });
      };
    }
  }
};

const handleCookieConsentClicks = e => {
  if (!e || !e.target) {
    return null;
  }
  const modalLink = checkIfDoNotSellLink(e.target);
  if (modalLink) {
    window.OneTrust?.ToggleInfoDisplay();
    const toggleEl = document.getElementById('ot-group-id-BG8');
    cookieConsentToggleState = toggleEl?.hasAttribute('checked') ? 'on' : 'off';
  } else if (e.target.matches('#ot-group-id-BG8')) {
    cookieConsentToggleState = e.target.hasAttribute('checked') ? 'on' : 'off';
  } else if (e.target.matches('.save-preference-btn-handler')) {
    trackCookieConsentClicks({
      event: 'send-VPV',
      'vpv-name': `/vpv/de/modal/cookie-settings/onclick/do-not-sell-or-share-my-personal-information/${formattedSiteName}/na/ca/${
        cookieConsentToggleState || 'na'
      }`,
    });
  }
  return null;
};

if (document.readyState === 'complete' || document.readyState === 'interactive') {
  removeDoNotSellRedirect();
} else {
  document.addEventListener('DOMContentLoaded', removeDoNotSellRedirect);
}

document.addEventListener('click', handleCookieConsentClicks);
</script>
