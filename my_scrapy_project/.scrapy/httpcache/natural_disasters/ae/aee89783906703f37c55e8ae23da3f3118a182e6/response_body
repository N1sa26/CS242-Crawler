        var check_site_id = 259;

        var social_patterns = ['facebook.com/share','twitter.com/share','linkedin.com/share','pinterest.com/share','mailto:?subject'];

        var ga_event = function(type,title,id){
        //console.log(type,title,id);
            try {
                    
                        if(type == "PublishDate")
                        {
                        if(check_site_id === 311){
                          ga('ipr.send', 'event', type, title, id,
                          {
                            'dimension3': 'nonIntraction',
                            'nonInteraction': 1
                          });
                        }
                        else
                        {
                          ga('ipr.send', 'event', type, title, id,
                          {
                            'nonInteraction': 1
                          });
                        }
                        }
                        else
                        {
                        ga('ipr.send', 'event', type, title, id);
                        }
                    
            }
            catch(e) {

            }
        }

        

            function ga_social_event(network,action,target){
                     ga('ipr.send', 'social', network, action, target);
                }

            ga('create', 'UA-70192924-1', {'name':'ipr','cookieName': 'ipr_ga'});

        

        

           

